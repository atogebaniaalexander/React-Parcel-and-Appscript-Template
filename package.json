{
  "name": "react-parcel-and-appscript-template",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "glogin": "clasp login",
    "glogout": "clasp logout",
    "gcreate": "clasp create --title 'React-Appscript-Project' --rootDir ./appScripts",
    "gpush": "clasp push",
    "gstart": "clasp push --watch",
